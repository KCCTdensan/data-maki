{
  "extends": ["@data-maki/typescript-config/base.json"],
  "compilerOptions": {
    "jsx": "react-jsx",
    "jsxImportSource": "hono/jsx",
    "paths": {
      "@/*": ["./src/*"]
    },
    "plugins": [
      {
        "transform": "typia/lib/transform"
      }
    ]
  },
  "include": ["src/**/*.ts"],
  "files": ["scripts/build.ts"]
}
